{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "node": true,
        "browser": true
    },
    "rules": {
        "no-console": 0
    }
    , "overrides": [
        {
            "files": ["dist/*/*.js"],
            "rules": {
                "require-jsdoc": "off",
                "no-sparse-arrays": "off"
            }
        }
    ]
}
